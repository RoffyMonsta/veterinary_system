    "start-uk": "ng serve --proxy-config proxy.conf.json --watch --configuration=uk",
    "build:ssr": "ng build --prod && gulp purifyCSS && ng run vetcabinet:server:production && cpx angular.json dist/vetcabinet && cpx proxy.server.js dist/vetcabinet"
  },
  "private": true,
